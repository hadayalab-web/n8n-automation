# n8n Cloud MCP Settings (for local Cursor setup)
N8N_API_URL=https://your-instance.app.n8n.cloud
N8N_API_KEY=your_api_key_here

# Vercel MCP Settings
VERCEL_ACCESS_TOKEN=your_vercel_token_here

# Google Workspace MCP Settings
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REFRESH_TOKEN=your_google_refresh_token_here

# PostgreSQL/Supabase MCP Settings
POSTGRES_CONNECTION_STRING=postgresql://user:password@host:port/database

# Note: These are configured in ~/.cursor/mcp.json (Windows: C:\Users\USERNAME\.cursor\mcp.json)